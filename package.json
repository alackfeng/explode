{
  "name": "explode",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "setup": "gulp setup",
    "start:gulp": "gulp -h",
    "ios": "react-native run-ios --device",
    "web": "npm run start:packager:web",
    "start:packager": "node node_modules/react-native/local-cli/cli.js start",
    "start:packager:web": "webpack-dev-server --config web/webpack.config.js --inline",
    "start:packager:macos": "node node_modules/react-native-macos/local-cli/cli.js start",
    "build:production:web": "cross-env NODE_ENV=production webpack --config web/webpack.config.js --progress",
    "build:debug:web": "webpack --config web/webpack.config.js --progress",
    "start:debug:server": "nodemon build/server/debug/main.js --ignore js/ --ignore web/ --ignore index.web.js",
    "start:production:web": "serve build/web/production -s -c 1",
    "test:native": "jest --config jest/jest.native.json",
    "test:web": "jest --config jest/jest.web.json",
    "test:jest": "jest --config jest/jest.native.json && jest --config jest/jest.web.json",
    "test": "jest",
    "lint": "eslint -c .eslintrc --fix js/**",
    "validate": "npm ls"
  },
  "dependencies": {
    "animated": "^0.2.1",
    "assetfunjs": "git+https://github.com/alackfeng/assetfunjs.git",
    "es6-promise": "^4.2.2",
    "humps": "^2.0.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "normalizr": "^3.2.4",
    "path-to-regexp": "^2.1.0",
    "react": "16.2.0",
    "react-dom": "^16.2.0",
    "react-motion": "^0.5.2",
    "react-native": "0.52.0",
    "react-native-clean-form": "file:../react-native-clean-form",
    "react-native-elements": "^1.0.0-beta",
    "react-native-i18n": "^2.0.10",
    "react-native-material-design-searchbar": "^1.8.0",
    "react-native-modal": "^4.1.1",
    "react-native-parallax-scroll-view": "^0.20.1",
    "react-native-search-bar": "^3.3.1",
    "react-native-stream": "^0.1.9",
    "react-native-vector-icons": "^4.5.0",
    "react-native-web": "^0.3.1",
    "react-navigation": "git+https://github.com/react-community/react-navigation.git#d5618ebd41ec1ac53cb40301d0f71610fbbb7172",
    "react-redux": "^5.0.6",
    "react-swipeable-views": "^0.12.12",
    "react-swipeable-views-native": "^0.12.9",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.4.0",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.2.0",
    "styled-components": "^2.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "22.1.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.12",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-native": "4.0.0",
    "babel-preset-stage-0": "^6.24.1",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "jest": "22.1.1",
    "react-test-renderer": "16.2.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
